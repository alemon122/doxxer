{
  "name": "doxxer-desktop",
  "version": "1.0.0",
  "description": "IP Logger Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . doxxer --platform=win32 --arch=x64 --out=dist/win --icon=assets/icon.ico --overwrite",
    "package-mac": "electron-packager . doxxer --platform=darwin --arch=x64 --out=dist/mac --icon=assets/icon.icns --overwrite",
    "package-linux": "electron-packager . doxxer --platform=linux --arch=x64 --out=dist/linux --icon=assets/icon.png --overwrite"
  },
