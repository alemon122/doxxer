  "dependencies": {
    "electron": "^22.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "axios": "^1.6.2",
    "connect-pg-simple": "^8.0.0",
    "date-fns": "^2.29.3",
    "postgres": "^3.3.2",
    "drizzle-orm": "^0.22.0",
    "drizzle-zod": "^0.4.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "electron-packager": "^17.1.1"
  }
